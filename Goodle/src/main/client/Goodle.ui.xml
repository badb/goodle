<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:g="urn:import:com.google.gwt.user.client.ui"
			 xmlns:my="urn:import:main.client.ui">
			 
	<ui:style src="../../css/Goodle.css"/>
 
	<g:DockLayoutPanel unit="PX" styleName='{style.mainPanelStyle}'>
		<g:north size="50">
			<my:TopView styleName='{style.topPanel}' ui:field="topPanel" />
		</g:north>
		<g:west size="200">
			<my:UserCoursesView styleName='{style.leftPanel}' ui:field="leftPanel" />
		</g:west>
		<g:center size="500">
			<g:SimpleLayoutPanel styleName='{style.contentPanel}' ui:field="contentPanel" />
		</g:center>
		<g:east size="200">
			<my:CalendarView styleName='{style.rightPanel}' ui:field="rightPanel" />
		</g:east>
		<g:south size="5">
		    <my:FooterView ui:field="footer" />
		</g:south>
	</g:DockLayoutPanel>
			 
			 
</ui:UiBinder>