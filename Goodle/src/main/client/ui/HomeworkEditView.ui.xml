<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:main.client.ui"
	xmlns:dp="urn:import:com.google.gwt.user.datepicker.client" 
> 
	<ui:style src="../resources/Goodle.css"/>
  
	<g:HTMLPanel styleName='{style.module}' width="100%">
	
		<g:Button ui:field="deleteButton" styleName='{style.rightButton}'>Usuń pracę domową</g:Button>
	    <my:EditLabel ui:field="title"/> 
	   	<g:CheckBox ui:field="isVisible" /> <br />
	    <my:EditLabel ui:field="text"  width="100%" /> <br />
	    <dp:DatePicker ui:field="deadline" /> <br />
	    <g:FlexTable ui:field="filesTable" width="100%" /> <br />
	    <my:FileUploadView ui:field="upload" width="100%" /> 
	</g:HTMLPanel>

</ui:UiBinder> 