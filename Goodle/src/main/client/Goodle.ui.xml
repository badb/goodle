<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:g="urn:import:com.google.gwt.user.client.ui"
			 xmlns:my="urn:import:main.client.ui">
			 
	<ui:style src="../../css/Goodle.css"/>
	<g:ScrollPanel>
	<g:VerticalPanel styleName='{style.mainPanelStyle}'> 
			<my:TopView styleName='{style.topPanel}' ui:field="topPanel" />
		<g:HorizontalPanel styleName='{style.mainUnderTopPanelStyle}'>
			<my:UserCoursesView styleName='{style.leftPanel}' ui:field="leftPanel" />
			<g:SimpleLayoutPanel styleName='{style.contentPanel}' ui:field="contentPanel" />
			<my:CalendarView styleName='{style.rightPanel}' ui:field="rightPanel" />
		</g:HorizontalPanel>
	</g:VerticalPanel>
</g:ScrollPanel>
</ui:UiBinder>