<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:main.client.ui">

    <g:HTMLPanel>
	<g:FormPanel ui:field="form"> 
	    <g:HorizontalPanel>
			<g:FileUpload ui:field="uploadField" name="file"/>
			<g:Label ui:field="info">Podaj opis pliku:</g:Label>
			<g:TextBox ui:field="title"></g:TextBox>
			<g:Button ui:field="submitButton">Upload</g:Button>
		</g:HorizontalPanel>
	</g:FormPanel>
	<br />	<g:Label ui:field="error" />
	</g:HTMLPanel>
	
</ui:UiBinder> 