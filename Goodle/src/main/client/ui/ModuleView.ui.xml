<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:my="urn:import:main.client.ui"
>
	<ui:style src="../resources/Goodle.css"/>
    
	<g:VerticalPanel width="100%" styleName='{style.module}'>
	    <g:Label ui:field="visible" width="100%"/>
	    <my:ResizableTextArea ui:field="title" readOnly="true" styleName="{style.textArea}" />
	    <my:ResizableTextArea ui:field="text" readOnly="true" styleName="{style.textArea}" />
	    <g:FlexTable ui:field="filesTable" width="100%"></g:FlexTable>
	</g:VerticalPanel>

</ui:UiBinder>