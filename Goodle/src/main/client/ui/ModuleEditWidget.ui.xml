<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
>
	<g:HorizontalPanel>
		<g:VerticalPanel>
			<g:DeckLayoutPanel ui:field="deckPanel">
			
				<g:Label ui:field="desc"></g:Label>
				<g:TextArea ui:field="descEdit" />
			</g:DeckLayoutPanel>
			<g:Button ui:field="editButton">Edytuj opis</g:Button>
		</g:VerticalPanel>
		
		<g:VerticalPanel>
			<g:CheckBox ui:field="checkBox">Udostępnij moduł studentom</g:CheckBox>
		
		</g:VerticalPanel>
	
	</g:HorizontalPanel>

</ui:UiBinder>