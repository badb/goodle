<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
<ui:style src="../resources/Goodle.css"/>

		<g:VerticalPanel styleName="{style.popUp}">
		<g:HorizontalPanel height="">
			<g:TextBox ui:field="searchBox"/>
			<g:Button ui:field="searchButton" text="Szukaj" styleName="{style.popUpButton}"/>
		</g:HorizontalPanel>
		<p1:CellList ui:field="cellList"/>
		<g:Cell verticalAlignment="ALIGN_BOTTOM" horizontalAlignment="ALIGN_RIGHT">
			<g:HorizontalPanel horizontalAlignment="ALIGN_RIGHT">
			<g:Button ui:field="cancelButton" text="Anuluj" styleName="{style.popUpButton}"/>
			</g:HorizontalPanel>
		</g:Cell>
		</g:VerticalPanel>

</ui:UiBinder> 