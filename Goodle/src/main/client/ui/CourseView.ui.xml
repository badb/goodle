<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
        xmlns:g="urn:import:com.google.gwt.user.client.ui"
        xmlns:my="urn:import:main.client.ui"
>

        <g:FlowPanel>
	        <g:Label ui:field="courseName" />
	        <g:TabLayoutPanel barUnit="PX" barHeight="20" width="300px" height="200px" ui:field="tabPanel">
	     	<g:tab>
				<g:header>Informacje</g:header>
				<my:MyLazyPanel>         
            		<my:CourseInfoView ui:field="courseInfoView"/>
            	</my:MyLazyPanel>
            </g:tab>
	        <g:tab>
	            <g:header>Modu≈Çy</g:header>
	            <my:MyLazyPanel>
	            	<my:CourseModulesView ui:field="courseModulesView" width="200px" />            
	            </my:MyLazyPanel>
	    	</g:tab>
	    	<g:tab>
				<g:header>Grupy</g:header>
				<my:MyLazyPanel>        
            		<my:CourseGroupsView ui:field="courseGroupsView"/>
            	</my:MyLazyPanel>
            </g:tab>
			<g:tab>
				<g:header>Uczestnicy</g:header>
				<my:MyLazyPanel>    
            		<my:CourseMembersView ui:field="courseMembersView"/>
            	</my:MyLazyPanel>
            </g:tab>
            <g:tab>
				<g:header>Ankiety</g:header>
				<my:MyLazyPanel>         
            		<my:CourseFormsView ui:field="courseFormsView"/>
            	</my:MyLazyPanel>
            </g:tab>
       </g:TabLayoutPanel>              
	</g:FlowPanel>
</ui:UiBinder>
