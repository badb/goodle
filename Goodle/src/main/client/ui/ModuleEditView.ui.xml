<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:main.client.ui"
>
	<ui:style src="../resources/Goodle.css"/>

	<g:HTMLPanel styleName='{style.module}'>
		<g:Button ui:field="deleteButton" styleName='{style.rightButton}'/>
	    <my:EditLabel ui:field="title" styleName='{style.moduleTitle}'/>
	   	<g:CheckBox ui:field="isVisible" styleName='{style.moduleVisible}'/> 
	    <my:EditLabel ui:field="text" wordWrapped="true" /> <br />
	    <g:FlexTable ui:field="filesTable" width="100%" /> <br />
	    <my:FileUploadView ui:field="upload" width="100%" /> 
	</g:HTMLPanel>

</ui:UiBinder> 