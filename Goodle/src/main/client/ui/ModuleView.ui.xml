<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
>
	<g:FocusPanel ui:field="focusPanel">
	<g:VerticalPanel width="100%">
		<g:Button ui:field="editButton">Edytuj moduł</g:Button>
		
		<g:HorizontalPanel ui:field="editPanel" visible="false" width="100%">
			<g:Button ui:field="saveButton">Zapisz zmiany</g:Button>
	    	<g:Button ui:field="cancelButton">Zakończ edycję bez zapisywania zmian</g:Button>
	    	<g:CheckBox ui:field="showHideBox">Pokaż moduł studentom</g:CheckBox>
	    </g:HorizontalPanel>
	    
		<g:DeckLayoutPanel ui:field="titleDeckPanel" width="100%" height="40px">
	    	<g:Label ui:field="titleView" />
	    	<g:TextBox ui:field="titleEdit" height="20px" />
	    </g:DeckLayoutPanel>
	    <g:DeckLayoutPanel ui:field="descDeckPanel" width="100%" height="40px">
	    			
			<g:Label ui:field="descView" />
			<g:TextBox ui:field="descEdit" height="20px"/>
		</g:DeckLayoutPanel>

		
	</g:VerticalPanel>
	</g:FocusPanel>

</ui:UiBinder>