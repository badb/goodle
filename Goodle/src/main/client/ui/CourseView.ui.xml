<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
        xmlns:g="urn:import:com.google.gwt.user.client.ui"
        xmlns:my="urn:import:main.client.ui"
>

        <g:FlowPanel>
	        <g:Label ui:field="courseName" />
	        <g:Label ui:field="courseDesc" />
	        <g:TabLayoutPanel barUnit="PX" barHeight="20" width="300px" height="200px" ui:field="tabPanel">
	        <g:tab>
	            <g:header>Lista modułów...</g:header>
	            <my:MyLazyPanel>
	                    <!-- <g:Label>Tutaj będzie lista modułów</g:Label> -->
	            <my:ModulesTabView ui:field="modulesTabView" width="200px"></my:ModulesTabView>
	            
	            </my:MyLazyPanel>
	    	</g:tab>
	        <g:tab>
				<g:header>Informacje o kursie</g:header>
				<my:MyLazyPanel>
           
           			<!-- <g:Label>.......</g:Label> -->
            		<my:CourseInfoView ui:field="courseInfoView"/>
            	</my:MyLazyPanel>
            </g:tab>
       </g:TabLayoutPanel>              
	</g:FlowPanel>
</ui:UiBinder>
