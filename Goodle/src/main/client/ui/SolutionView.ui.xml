<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:main.client.ui">
	
<ui:style src="../resources/Goodle.css"/>

	<g:VerticalPanel>
		<my:FileView ui:field="fileView" width="100%"/>
		<g:HorizontalPanel ui:field="markPanel" visible="false">
			<g:Label text="Komentarz: " />
			<my:EditLabel ui:field="comment" editable="false" wordWrapped="true" />
			<g:Label text="Ocena: " />
			<g:TextBox ui:field="mark" enabled="false" />
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 