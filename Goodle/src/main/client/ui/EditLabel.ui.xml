<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:main.client.ui">

	<ui:style src="../resources/Goodle.css"/>
	<g:HTMLPanel>
		<g:HTML ui:field="label" ui:text="Edytuj" />
		<g:TextBox ui:field="textBox" visible="false" maxLength="20000" />
		<my:ResizableTextArea ui:field="textArea" visible="false" maxLength="20000" styleName='{style.textArea}' /> <br />
		<g:Button ui:field="save" visible="false" styleName='{style.leftButton}' ui:text="Zapisz" />	
		<g:Button ui:field="cancel" visible="false" styleName="{style.leftButton}" ui:text="Anuluj" /> <br />
	</g:HTMLPanel>
</ui:UiBinder> 