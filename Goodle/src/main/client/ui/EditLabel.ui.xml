<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:main.client.ui">

	<ui:style src="../resources/Goodle.css"/>
	<g:VerticalPanel>
				<my:ResizableTextArea ui:field="textArea" width="90%" styleName='{style.textArea}' />
				<g:HorizontalPanel>
					<g:Button ui:field="saveButton" styleName='{style.leftButton}'>Zapisz</g:Button>	
					<g:Button ui:field="cancelButton" styleName="{style.leftButton}">Anuluj</g:Button>	
				</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 