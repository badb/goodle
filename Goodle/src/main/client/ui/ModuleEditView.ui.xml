<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:main.client.ui"
>
	<ui:style src="../resources/Goodle.css"/>

	<g:VerticalPanel width="100%" styleName='{style.module}'>
	    <my:EditLabel width="100%" ui:field="title" />
	   	<g:CheckBox ui:field="isVisible" />
	    <my:EditLabel ui:field="text" width="100%"/>
	    <g:FlexTable ui:field="filesTable" width="100%"></g:FlexTable>
	    <my:FileUploadView ui:field="upload" width="100%"></my:FileUploadView>
	</g:VerticalPanel>

</ui:UiBinder> 